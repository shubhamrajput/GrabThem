{
   "name":"GrabThem",
   "version":"0.0.1",
   "manifest_version":2,
   "description":"Save all the url(s) of the open tabs.",
   "homepage_url":"https://shubhamrajput.github.io/",
   "icons":{
      "16":"icons/icon16.png",
      "48":"icons/icon48.png",
      "128":"icons/icon128.png"
   },
   "browser_action":{
      "default_icon":"icons/icon19.png",
      "default_title":"Save the tabs",
      "default_popup":"popup.html"
   },
   "permissions":[
      "tabs",
      "storage"
   ]
}